{
    "welcomeFile": "index.html",
    "logout": {
        "logoutEndpoint": "/logout",
        "logoutPage": "/"
    },
    "routes": [{
        "source": "^/registerJob",
        "destination": "jsmt-sample-core",
        "httpMethods": ["GET"],
        "scope": "$XSAPPNAME.User"
    }, {
        "source": "^/getJobs",
        "destination": "jsmt-sample-core",
        "httpMethods": ["GET"],
        "scope": "$XSAPPNAME.User"
    }, {
        "source": "^/(.*)$",
        "localDir": "resources"
    }]
}